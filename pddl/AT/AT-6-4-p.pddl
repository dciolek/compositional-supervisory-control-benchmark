(define
  (problem Goal)
  (:domain DirectedController)
  (:init
    (at $0 $HeightMonitor-1)
    (at $0 $Airplane-4)
    (at $0 $Airplane-5)
    (at $0 $Airplane-2)
    (at $0 $Airplane-1)
    (at $0 $HeightMonitor-2)
    (at $0 $Airplane-3)
    (at $0 $HeightMonitor-0)
    (at $0 $ResponseMonitor)
    (at $0 $RampMonitor)
    (at $0 $HeightMonitor-3)
    (at $0 $Airplane-0)
  )
  (:goal
    (and
      (status event)
      (hoop $HeightMonitor-1)
      (hoop $Airplane-4)
      (hoop $Airplane-5)
      (hoop $Airplane-2)
      (hoop $Airplane-1)
      (hoop $HeightMonitor-2)
      (hoop $Airplane-3)
      (hoop $HeightMonitor-0)
      (hoop $ResponseMonitor)
      (hoop $RampMonitor)
      (hoop $HeightMonitor-3)
      (hoop $Airplane-0)
    )
  )
)
